/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=notSel);
    And(a=a,b=notSel,out=c);
	And(a=b,b=sel,out=d);
	Or(a=c,b=d,out=out);

}