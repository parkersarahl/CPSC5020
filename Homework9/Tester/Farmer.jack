class Farmer {
    field int location;
    
    constructor Farmer new(int x){
        let location = x;

        do draw();

        return this;
    }
     
    method void draw() {
        do Graphics.drawFarmer(location);
        return;
    }
	
    method void dispose(){
        do Memory.deAlloc(this);
        return;
    }

    method void move(){
        do Graphics.moveFarmer(location -2);
        return;
    }
}